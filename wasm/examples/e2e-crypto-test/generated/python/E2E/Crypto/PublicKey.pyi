from __future__ import annotations

import flatbuffers
import numpy as np

import typing

uoffset: typing.TypeAlias = flatbuffers.number_types.UOffsetTFlags.py_type

class PublicKey(object):
  @classmethod
  def GetRootAs(cls, buf: bytes, offset: int) -> PublicKey: ...
  @classmethod
  def GetRootAsPublicKey(cls, buf: bytes, offset: int) -> PublicKey: ...
  @classmethod
  def PublicKeyBufferHasIdentifier(cls, buf: bytes, offset: int, size_prefixed: bool) -> bool: ...
  def Init(self, buf: bytes, pos: int) -> None: ...
  def Data(self, i: int) -> typing.List[int]: ...
  def DataAsNumpy(self) -> np.ndarray: ...
  def DataLength(self) -> int: ...
  def DataIsNone(self) -> bool: ...
  def Length(self) -> int: ...
def PublicKeyStart(builder: flatbuffers.Builder) -> None: ...
def Start(builder: flatbuffers.Builder) -> None: ...
def PublicKeyAddData(builder: flatbuffers.Builder, data: uoffset) -> None: ...
def PublicKeyStartDataVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def StartDataVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def PublicKeyAddLength(builder: flatbuffers.Builder, length: int) -> None: ...
def PublicKeyEnd(builder: flatbuffers.Builder) -> uoffset: ...
def End(builder: flatbuffers.Builder) -> uoffset: ...

