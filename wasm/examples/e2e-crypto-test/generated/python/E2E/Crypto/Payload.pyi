from __future__ import annotations

import flatbuffers
import numpy as np

import typing

uoffset: typing.TypeAlias = flatbuffers.number_types.UOffsetTFlags.py_type

class Payload(object):
  @classmethod
  def GetRootAs(cls, buf: bytes, offset: int) -> Payload: ...
  @classmethod
  def GetRootAsPayload(cls, buf: bytes, offset: int) -> Payload: ...
  @classmethod
  def PayloadBufferHasIdentifier(cls, buf: bytes, offset: int, size_prefixed: bool) -> bool: ...
  def Init(self, buf: bytes, pos: int) -> None: ...
  def Message(self) -> str | None: ...
  def Value(self) -> int: ...
  def Data(self, i: int) -> typing.List[int]: ...
  def DataAsNumpy(self) -> np.ndarray: ...
  def DataLength(self) -> int: ...
  def DataIsNone(self) -> bool: ...
  def Nested(self, i: int) -> Payload | None: ...
  def NestedLength(self) -> int: ...
  def NestedIsNone(self) -> bool: ...
  def IsEncrypted(self) -> bool: ...
def PayloadStart(builder: flatbuffers.Builder) -> None: ...
def Start(builder: flatbuffers.Builder) -> None: ...
def PayloadAddMessage(builder: flatbuffers.Builder, message: uoffset) -> None: ...
def PayloadAddValue(builder: flatbuffers.Builder, value: int) -> None: ...
def PayloadAddData(builder: flatbuffers.Builder, data: uoffset) -> None: ...
def PayloadStartDataVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def StartDataVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def PayloadAddNested(builder: flatbuffers.Builder, nested: uoffset) -> None: ...
def PayloadStartNestedVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def StartNestedVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def PayloadAddIsEncrypted(builder: flatbuffers.Builder, isEncrypted: bool) -> None: ...
def PayloadEnd(builder: flatbuffers.Builder) -> uoffset: ...
def End(builder: flatbuffers.Builder) -> uoffset: ...

