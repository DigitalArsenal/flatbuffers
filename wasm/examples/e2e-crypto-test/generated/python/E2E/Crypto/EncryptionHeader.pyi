from __future__ import annotations

import flatbuffers
import numpy as np

import typing
from E2E.Crypto.Curve import Curve
from E2E.Crypto.EncryptionAlgorithm import EncryptionAlgorithm
from E2E.Crypto.KeyExchangeAlgorithm import KeyExchangeAlgorithm
from E2E.Crypto.PublicKey import PublicKey

uoffset: typing.TypeAlias = flatbuffers.number_types.UOffsetTFlags.py_type

class EncryptionHeader(object):
  @classmethod
  def GetRootAs(cls, buf: bytes, offset: int) -> EncryptionHeader: ...
  @classmethod
  def GetRootAsEncryptionHeader(cls, buf: bytes, offset: int) -> EncryptionHeader: ...
  @classmethod
  def EncryptionHeaderBufferHasIdentifier(cls, buf: bytes, offset: int, size_prefixed: bool) -> bool: ...
  def Init(self, buf: bytes, pos: int) -> None: ...
  def Version(self) -> int: ...
  def KeyExchange(self) -> typing.Literal[KeyExchangeAlgorithm.ECDH]: ...
  def Curve(self) -> typing.Literal[Curve.X25519, Curve.Secp256k1, Curve.P256]: ...
  def Encryption(self) -> typing.Literal[EncryptionAlgorithm.AES_256_CTR]: ...
  def EphemeralPublicKey(self) -> PublicKey | None: ...
  def SenderId(self) -> str | None: ...
  def Timestamp(self) -> int: ...
def EncryptionHeaderStart(builder: flatbuffers.Builder) -> None: ...
def Start(builder: flatbuffers.Builder) -> None: ...
def EncryptionHeaderAddVersion(builder: flatbuffers.Builder, version: int) -> None: ...
def EncryptionHeaderAddKeyExchange(builder: flatbuffers.Builder, keyExchange: typing.Literal[KeyExchangeAlgorithm.ECDH]) -> None: ...
def EncryptionHeaderAddCurve(builder: flatbuffers.Builder, curve: typing.Literal[Curve.X25519, Curve.Secp256k1, Curve.P256]) -> None: ...
def EncryptionHeaderAddEncryption(builder: flatbuffers.Builder, encryption: typing.Literal[EncryptionAlgorithm.AES_256_CTR]) -> None: ...
def EncryptionHeaderAddEphemeralPublicKey(builder: flatbuffers.Builder, ephemeralPublicKey: uoffset) -> None: ...
def EncryptionHeaderAddSenderId(builder: flatbuffers.Builder, senderId: uoffset) -> None: ...
def EncryptionHeaderAddTimestamp(builder: flatbuffers.Builder, timestamp: int) -> None: ...
def EncryptionHeaderEnd(builder: flatbuffers.Builder) -> uoffset: ...
def End(builder: flatbuffers.Builder) -> uoffset: ...

