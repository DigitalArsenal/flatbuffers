from __future__ import annotations

import flatbuffers
import numpy as np

import typing
from E2E.Crypto.Payload import Payload

uoffset: typing.TypeAlias = flatbuffers.number_types.UOffsetTFlags.py_type

class SecureMessage(object):
  @classmethod
  def GetRootAs(cls, buf: bytes, offset: int) -> SecureMessage: ...
  @classmethod
  def GetRootAsSecureMessage(cls, buf: bytes, offset: int) -> SecureMessage: ...
  @classmethod
  def SecureMessageBufferHasIdentifier(cls, buf: bytes, offset: int, size_prefixed: bool) -> bool: ...
  def Init(self, buf: bytes, pos: int) -> None: ...
  def Id(self) -> str | None: ...
  def Sender(self) -> str | None: ...
  def Recipient(self) -> str | None: ...
  def Payload(self) -> Payload | None: ...
  def Timestamp(self) -> int: ...
  def Signature(self, i: int) -> typing.List[int]: ...
  def SignatureAsNumpy(self) -> np.ndarray: ...
  def SignatureLength(self) -> int: ...
  def SignatureIsNone(self) -> bool: ...
def SecureMessageStart(builder: flatbuffers.Builder) -> None: ...
def Start(builder: flatbuffers.Builder) -> None: ...
def SecureMessageAddId(builder: flatbuffers.Builder, id: uoffset) -> None: ...
def SecureMessageAddSender(builder: flatbuffers.Builder, sender: uoffset) -> None: ...
def SecureMessageAddRecipient(builder: flatbuffers.Builder, recipient: uoffset) -> None: ...
def SecureMessageAddPayload(builder: flatbuffers.Builder, payload: uoffset) -> None: ...
def SecureMessageAddTimestamp(builder: flatbuffers.Builder, timestamp: int) -> None: ...
def SecureMessageAddSignature(builder: flatbuffers.Builder, signature: uoffset) -> None: ...
def SecureMessageStartSignatureVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def StartSignatureVector(builder: flatbuffers.Builder, num_elems: int) -> uoffset: ...
def SecureMessageEnd(builder: flatbuffers.Builder) -> uoffset: ...
def End(builder: flatbuffers.Builder) -> uoffset: ...

